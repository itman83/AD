.classifiedd-news-category label {
	clear: left;
    display: block;
    float: left;
    font-size: 13px;
    font-weight: bold;
    margin: 15px 0px 0px 0px;
    padding: 3px 10px 0 0;
    text-align: right;
    width: 170px !important;
}

.classifiedd-news-category label span{
	clear: left;
    display: block;
    float: left;
    font-size: 11px;
    font-weight: normal;
    margin: 5px 0px 0px 0px;
    padding: 3px 10px 0 0;
    text-align: right;
    width: 170px !important;
}

.classifiedd-news-category .btn{
	background: url("../img/search-area-bg.png") repeat scroll 0 0 transparent;
	font-size:18px;
	text-align:center;
	float:left;
	color:#FFF;
	border:0px;
	border-radius:10px;
	padding:5px 10px;
	cursor:pointer;
	clear:right;
	margin-top:12px;
}

.classifiedd-news-category .form1 {
	float:left;
	margin:20px 0px 20px 0px;
}

.classifiedd-news-category input[type="text"] {
	border: 1px solid #CCCCCC;
    border-radius: 3px 3px 3px 3px;
    box-shadow: 0 0 5px #CCCCCC inset;
    font-size: 13px;
    height: 35px;
    padding: 4px 5px;
    width: 300px;
	float:left;
	margin:10px 0px 5px 0px;
}

.classifiedd-news-category textarea {
	border: 1px solid #CCCCCC;
    border-radius: 3px 3px 3px 3px;
    box-shadow: 0 0 5px #CCCCCC inset;
    font-size: 13px;
    height: 90px;
    padding: 4px 5px;
    width: 300px;
	float:left;
	margin:10px 0px 5px 0px;
}

address {
	float:left;
	margin:30px 0px 0px 100px;
	padding:0px;
	width:400px;
	font-style:normal;
	font-family:Arial, Helvetica, sans-serif
}

address h1 {
	float:left;
	margin:0px auto;
	padding:0px;
	width:400px;
	font-family:Arial, Helvetica, sans-serif;
	font-size:20px;
	font-style:normal;
	font-weight:normal
}

address p {
	float:left;
	margin:15px 0px 0px 0px !important;
	padding:0px;
	width:400px;
	font-family:Arial, Helvetica, sans-serif;
	font-size:14px !important;
	font-style:normal;
	font-weight:normal;
	color:#000 !important
}

address p span{
	float:left;
	margin:0px 0px 0px 0px !important;
	padding:0px;
	width:65px;
	font-family:Arial, Helvetica, sans-serif;
	font-size:14px !important;
	font-style:normal;
	font-weight:normal;
	color:#000 !important;
	line-height:1.5em !important;
	font-weight:bold
}

.box-sh-none{
	box-shadow: 0 0 0px rgba(0, 0, 0, 0.2)!important;
	}


.search-btn{
	background-color: #343434;
    background-image: linear-gradient(#343434, #131313);
    border: 0 solid #343434;
    border-radius: 5px 5px 5px 5px;
    color: #FFFFFF;
    cursor: pointer;
    float: left;
    font-family: Arial,Helvetica,sans-serif !important;
    font-size: 15px;
    margin: 12px 0 0 15px;
    overflow: visible;
    padding: 6px 22px;
    transition: all 0.3s ease 0s;
    width: auto;
}


