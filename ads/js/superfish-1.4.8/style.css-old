
body,
div,
h1,
h2,
h3,
h4,
h5,
h6,
p,
blockquote,
pre,
dl,
dt,
dd,
ol,
ul,
li,
fieldset,
form,
label,
legend,
th,
td,
article,
aside,
figure,
footer,
header,
hgroup,
menu,
nav,
section {
  margin: 0;
  padding: 0;
  border: 0;
}

html {
  height: 100%;
  -webkit-text-size-adjust: 100%;
  -ms-text-size-adjust: 100%;
}

body {
  min-height: 100%;
  font-size: 100%;
}

article,
aside,
details,
figcaption,
figure,
footer,
header,
hgroup,
nav,
section,
audio,
canvas,
video {
  display: block;
}
	
header {
	margin:0px;
	padding:0px;
	border-bottom:1px solid #cccccc;
	}
	
.header-inner {
	width:80%;
	margin:0px auto;
	padding:0px;
	}
	
.header-top {
	width:100%;
	margin:0px;
	padding:0px;
	float:left;
	}
	
.logo-area {
	width:30%;
	margin:0px;
	padding:20px 0px 0px 0px;
	float:left;
	}
	
.logo-area img {
	max-width:100%;
	}
	
.header-nav {
	width:42%;
	margin:0px;
	padding:30px 0px 0px 0px;
	float:left;
	}
	
.header-nav ul {
	margin:0px;
	padding:0px;
	list-style:none;
	float:right;
	}
	
.header-nav ul li {
	margin:0px;
	padding:0px;
	float:left;
	border-right:1px #000 solid;
	height:15px;
	}
	
.header-nav ul li:last-child {
	border-right:none;
	}

.header-nav ul li a {
	margin:0px 10px;
	padding:0px;
	text-decoration:none;
	color:#000;
	font-family:Arial, Helvetica, sans-serif;
	font-size:12px;
	vertical-align:top
	
	}
	
.social-area {
	width:27%;
	margin:0px;
	padding:25px 0px 0px 0px;
	float:right;
	}
	
.social-area img {
	margin:0px 0px 0px 10px;
	padding:0px;
	float:right;
	width:30px
	}
	

/*---------------search section ----------------*/	
	
.red-search-section {
